<div class="container">
  <h1 class="title"> Add a new tree</h1>
  <%= simple_form_for @tree do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :fruit_type %>
    <%= f.input :price_per_year %>
    <%= f.input :quantity_per_year %>
    <%= f.button :submit %>
  <% end %>
</div>
